<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.daivustore.ui.home.HomeFragment"
        android:label="@string/title_catalago"
        tools:layout="@layout/fragment_produtos" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.daivustore.ui.dashboard.DashboardFragment"
        android:label="@string/title_carrinho"
        tools:layout="@layout/fragment_perfil" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.daivustore.ui.notifications.NotificationsFragment"
        android:label="@string/title_perfil"
        tools:layout="@layout/fragment_carrinho" />
</navigation>